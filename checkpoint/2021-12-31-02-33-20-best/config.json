{
    "model": "smp_unet",
    "pretrain": "efficientnet-b1",
    "init_filter": 32,
    "depth": 4,
    "dropout": 0.2,
    "mish": 0,
    "batch_size": 8,
    "epoch": 100,
    "loss": "DFL",
    "optim": "adamw",
    "lr": 0.0005,
    "weight_decay": 0.0001,
    "scheduler": "cos",
    "step_size": 1,
    "gamma": 0.95,
    "device": "cuda:0",
    "save_root": "checkpoint",
    "weight_num": 10,
    "dataset": 2,
    "fliplr": 0.5,
    "flipud": 0.25,
    "rot90": 0,
    "bright": 0.1,
    "noise": 0.1
}